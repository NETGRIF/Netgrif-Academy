<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://netgrif.github.io/petriflow/petriflow.schema.xsd">
	<id>list</id>
	<initials>LST</initials>
	<title>New list</title>
	<icon>view_list</icon>
	<defaultRole>true</defaultRole>
	<anonymousRole>true</anonymousRole>
	<transitionRole>false</transitionRole>
	<role>
		<id>system</id>
		<title>System</title>
	</role>
	<data type="text">
		<id>text_0</id>
		<title>By clicking add, new case will be created.</title>
	</data>
	<data type="button">
		<id>button_0</id>
		<title/>
		<placeholder>Add</placeholder>
		<event type="set">
			<id>button_0_set</id>
			<actions phase="pre">
				<action id="1">taskRef_0: f.taskRef_0;
def myCase = createCase("element", "New element", "blue", userService.loggedOrSystem);
def myTask = findTask(myCase.tasks.find{it.transition == "t1"}.task);
def list = taskRef_0.value
list.add(myTask.stringId)
change taskRef_0 value { list; }</action>
			</actions>
		</event>
	</data>
	<data type="taskRef">
		<id>taskRef_0</id>
		<title/>
	</data>
	<data type="taskRef">
		<id>taskRef_1</id>
		<title/>
		<init>t2</init>
	</data>
	<data type="button">
		<id>button_1</id>
		<title/>
		<placeholder>Generate</placeholder>
		<event type="set">
			<id>button_1_set</id>
			<actions phase="pre">
				<action id="2">file_0: f.file_0;
generatePDF("t2", file_0.importId)</action>
			</actions>
		</event>
	</data>
	<data type="file">
		<id>file_0</id>
		<title>Generated file from form task</title>
	</data>
	<transition>
		<id>t1</id>
		<x>460</x>
		<y>100</y>
		<label>List of users</label>
		<roleRef>
			<id>default</id>
			<logic>
				<perform>true</perform>
			</logic>
		</roleRef>
		<dataGroup>
			<id>t1_0</id>
			<cols>4</cols>
			<layout>grid</layout>
			<dataRef>
				<id>text_0</id>
				<logic>
					<behavior>visible</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>0</y>
					<rows>1</rows>
					<cols>3</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
			<dataRef>
				<id>button_0</id>
				<logic>
					<behavior>editable</behavior>
				</logic>
				<layout>
					<x>3</x>
					<y>0</y>
					<rows>1</rows>
					<cols>1</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
			<dataRef>
				<id>taskRef_1</id>
				<logic>
					<behavior>editable</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>2</y>
					<rows>1</rows>
					<cols>4</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
			<dataRef>
				<id>button_1</id>
				<logic>
					<behavior>editable</behavior>
				</logic>
				<layout>
					<x>3</x>
					<y>1</y>
					<rows>1</rows>
					<cols>1</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
			<dataRef>
				<id>file_0</id>
				<logic>
					<behavior>visible</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>1</y>
					<rows>1</rows>
					<cols>3</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
		</dataGroup>
		<event type="assign">
			<id>t1_assign</id>
		</event>
		<event type="finish">
			<id>t1_finish</id>
		</event>
		<event type="cancel">
			<id>t1_cancel</id>
		</event>
		<event type="delegate">
			<id>t1_delegate</id>
		</event>
	</transition>
	<transition>
		<id>t2</id>
		<x>340</x>
		<y>100</y>
		<label>Form</label>
		<roleRef>
			<id>system</id>
			<logic>
				<perform>true</perform>
			</logic>
		</roleRef>
		<dataGroup>
			<id>t2_0</id>
			<cols>4</cols>
			<layout>grid</layout>
			<dataRef>
				<id>taskRef_0</id>
				<logic>
					<behavior>editable</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>0</y>
					<rows>1</rows>
					<cols>4</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
		</dataGroup>
		<event type="assign">
			<id>t2_assign</id>
		</event>
		<event type="finish">
			<id>t2_finish</id>
		</event>
		<event type="cancel">
			<id>t2_cancel</id>
		</event>
		<event type="delegate">
			<id>t2_delegate</id>
		</event>
	</transition>
</document>